<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!--#region Light Theme-->
    <Color x:Key="PrimaryLight">#11A98E</Color>
    <Color x:Key="SecondaryLight">#484F58</Color>
    <Color x:Key="TertiaryLight">#F0F0F0</Color>
    <Color x:Key="QuaternaryLight">#C5FBFF</Color>

    <Color x:Key="QuinaryLight">#13231F</Color>
    <Color x:Key="SextenaryLight">#98C2B8</Color>
    <Color x:Key="PlaceholderColorLight">#627D77</Color>

    <Color x:Key="BackgroundColorMainLight">#124037</Color>
    <Color x:Key="SurfaceColorLight">#FAFAFA</Color>

    <Color x:Key="NavigationBarColorLight">White</Color>
    <Color x:Key="BrandColorLight">#11A98E</Color>
    <!--#endregion-->

    <!--#region Dark Theme-->
    <Color x:Key="PrimaryDark">#23A05D</Color>
    <Color x:Key="SecondaryDark">White</Color>
    <Color x:Key="TertiaryDark">#B785F5</Color>
    <Color x:Key="QuaternaryDark">#C5FBFF</Color>

    <Color x:Key="QuinaryDark">#13231F</Color>
    <Color x:Key="SextenaryDark">#98C2B8</Color>
    <Color x:Key="PlaceholderColorDark">#627D77</Color>

    <Color x:Key="BackgroundColorMainDark">#124037</Color>
    <Color x:Key="SurfaceColorDark">#1D1E24</Color>

    <Color x:Key="NavigationBarColorDark">#111111</Color>
    <Color x:Key="BrandColorDark">White</Color>
    <!--#endregion-->

</ResourceDictionary>